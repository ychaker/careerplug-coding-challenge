<!DOCTYPE html>
<html>
  <head>
    <title>CodeingChallenge</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render 'header' %>

    <div id="alerts" class="">
      <%= render 'alerts' %>
    </div>

    <main>
      <div class="container">
        <%= yield %>
      </div>
    </main>
  </body>
</html>
